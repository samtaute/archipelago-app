{
  "name": "archipelago-app",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://samtaute.github.io/archipelago-app/",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
